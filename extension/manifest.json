{
  "manifest_version": 3,
  "name": "Auto Login ProtheusWEB",
  "version": "1.0.0",
  "description": "Auto Login do ProtheusWEB.",
  "permissions": [
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  },
  "content_scripts": [
    {
      "all_frames": true ,
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
  }
}